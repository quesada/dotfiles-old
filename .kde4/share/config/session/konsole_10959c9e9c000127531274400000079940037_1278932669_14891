[1]
Active=1
Default Profile[$e]=$HOME/.kde4/share/apps/konsole/Shell.profile
Sessions=1,2,3,4

[Number]
NumberOfSessions=4
NumberOfWindows=1

[Session1]
Encoding=UTF-8
LocalTab=%d : %n
Profile[$e]=$HOME/.kde4/share/apps/konsole/Shell.profile
RemoteTab=%h : %u
WorkingDir[$e]=$HOME

[Session2]
Encoding=UTF-8
LocalTab=%d : %n
Profile[$e]=$HOME/.kde4/share/apps/konsole/Shell.profile
RemoteTab=%h : %u
WorkingDir[$e]=/data

[Session3]
Encoding=UTF-8
LocalTab=%d : %n
Profile[$e]=$HOME/.kde4/share/apps/konsole/Shell.profile
RemoteTab=%h : %u
WorkingDir[$e]=$HOME/.ssh

[Session4]
Encoding=UTF-8
LocalTab=%d : %n
Profile[$e]=$HOME/.kde4/share/apps/konsole/Shell.profile
RemoteTab=%h : %u
WorkingDir[$e]=$HOME

