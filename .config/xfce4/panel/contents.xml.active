    <Group>
      <Control id="-1" filename="libseparator.so"/>
    </Group>
    <Group>
      <Control id="-1" filename="libsystembuttons.so" button1="0" button2="1" showtwo="1"/>
    </Group>
    <Group>
      <Control id="-1" filename="libseparator.so"/>
